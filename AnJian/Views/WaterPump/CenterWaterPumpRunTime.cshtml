
@{
    Layout = null;
}



<div style="text-align: center;display: inline-block">
    <span style="display: block">@ViewBag.Title</span>
    <div style="border:1px solid #B5B5B5;;display:inline-block;background-color:white">
        <div style="border: 1px solid #B5B5B5; margin: 5px; display: table">
            <div style="margin:5px;">
                <div style="display:inline-block;text-align: center;width:80px"> 水仓&nbsp;&nbsp;&nbsp;液位 </div>
                <div style=" display:inline-block; border:1px solid #787878 ;height:20px;width:50px;color:deepskyblue">@ViewBag.sumpLevel</div>
                <div style="display:inline-block;text-align: center;width:40px;text-align:left">@ViewBag.H </div>
            </div>
            <div style="margin:5px">
                <div style="display:inline-block;text-align: center;width:80px"> 主管路流量 </div>
                <div style=" display:inline-block; border:1px solid #787878 ;height:20px;width:50px;color:deepskyblue">@ViewBag.mainFlow</div>
                <div style="display:inline-block;text-align: center;width:40px;text-align:left">@ViewBag.F </div>
            </div>
            <div style="margin:5px">
                <div style="display:inline-block;text-align: center;width:80px"> 备管路流量 </div>
                <div style=" display:inline-block; border:1px solid #787878 ;height:20px;width:50px;color:deepskyblue ">@ViewBag.sublineFlow</div>
                <div style="display:inline-block;text-align: center;width:40px;text-align:left">@ViewBag.F </div>
            </div>
        </div>
    </div>
</div>
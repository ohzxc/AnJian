

@{
    Layout = null;
}

<div style="text-align: center;display: inline-block;">
    <span style="display: block">@ViewBag.Title</span>
    <div style="border:1px solid #B5B5B5;display:inline-block;background-color:white">
        <div style="border: 1px solid #B5B5B5; margin: 5px; display: table">
            <div style="margin:5px;">
                <div style="display:inline-block;text-align: center;width:80px"> 电机电压 </div>
                <div style=" display:inline-block; border:1px solid #787878 ;height:20px;width:50px;color:deepskyblue">@ViewBag.motorVoltsge</div>
                <div style="display:inline-block;text-align: center;width:20px;text-align:left"> @ViewBag.V </div>
            </div>
            <div style="margin:5px">
                <div style="display:inline-block;text-align: center;width:80px"> 电机温度 </div>
                <div style=" display:inline-block; border:1px solid #787878 ;height:20px;width:50px;color:deepskyblue">@ViewBag.motorTemperature</div>
                <div style="display:inline-block;text-align: center;width:20px;text-align:left">@ViewBag.T </div>
            </div>
            <div style="margin:5px">
                <div style="display:inline-block;text-align: center;width:80px"> 前轴温度 </div>
                <div style=" display:inline-block; border:1px solid #787878 ;height:20px;width:50px;color:deepskyblue">@ViewBag.frontTemperature</div>
                <div style="display:inline-block;text-align: center;width:20px;text-align:left">@ViewBag.T </div>
            </div>
            <div style="margin:5px">
                <div style="display:inline-block;text-align: center;width:80px"> 后轴温度 </div>
                <div style=" display:inline-block; border:1px solid #787878 ;height:20px;width:50px;color:deepskyblue">@ViewBag.rearTemperature</div>
                <div style="display:inline-block;text-align: center;width:20px;text-align:left">@ViewBag.T </div>
            </div>
            <div style="margin:5px">
                <div style="display:inline-block;text-align: center;width:80px"> 定子温度 </div>
                <div style=" display:inline-block; border:1px solid #787878 ;height:20px;width:50px;color:deepskyblue">@ViewBag.statorTemperature</div>
                <div style="display:inline-block;text-align: center;width:20px;text-align:left">@ViewBag.T </div>
            </div>
        </div>
    </div>
</div>